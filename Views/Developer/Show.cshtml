@model IEnumerable<ReleaseManagementMVC.Models.Module>

@{
    Layout = "~/Views/Shared/_ViewLayout.cshtml";}

<h2>Module History</h2>
<h2>Developer ID :  @TempData.Peek("EmployeeKey")</h2>
<h2>Developer Name : @TempData.Peek("EmployeeKeyName")</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ModuleID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ProjectID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TesterID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ModuleName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ModuleStatus)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ModuleID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ProjectID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TesterID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ModuleName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ModuleStatus)
            </td>

        </tr>
    }

</table>
<br />
<br />
<br />
@Html.ActionLink("Home", "WelcomeDev", "Developer")
@Html.ActionLink("View Modules", "Show", "Developer")
@Html.ActionLink("Mark as done", "DevMarkDone", "Developer")
@Html.ActionLink("View Bugs", "ViewBugs", "Developer")
@Html.ActionLink("Log out", "Logout", "Login")

